    (function() {
  // has to be the same as in lib/translations.rb
  // and in script/translations/update_translations
  var normalizeWhitespace = function(str) {
    return str.replace(/(\s\n)+/g, " ");
  };

  var _ = function(phrase) {
    normalizedPhrase = normalizeWhitespace(phrase);
    return _.msgs[normalizedPhrase] || normalizedPhrase;
  };

  var k_ = function(templateKey) {
    normalizedPhrase = normalizeWhitespace(templateKey);
    return k_.msgs[normalizedPhrase] || normalizedPhrase;
  };

  _.msgs = {};

  window.i18n = _;

